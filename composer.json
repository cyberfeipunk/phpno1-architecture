    "minimum-stability": "dev",
    "require": {
        "php":">=7.1"
    },
    "require-dev":{
        "phpunit/phpunit": "~6.0"
    },
    "autoload":{
        "psr-4":{
            "App\\": "app/",
            "Phpno1\\Architecture\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpno1\\Architecture\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Phpno1\\Architecture\\Providers\\ArchitectureServiceProvider"
            ]
        }
    }
}
